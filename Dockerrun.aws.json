{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "carvis-api",
      "image": "alexcstark/carvis-api",
      "essential": true,
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "node-app-template",
          "containerPath": "/var/opt/app",
          "readOnly": true
        }
      ],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8000
        }
      ]
    }
  ]
}
